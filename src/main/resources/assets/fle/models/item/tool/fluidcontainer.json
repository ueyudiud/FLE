{
	"textures": {
		"bottom": "[fle:fluidcontainer/bottom",
		"fluid": "[forge:fluid",
		"empty": "farcore:items/void",
		"convert": "[fle:fluidcontainer/convert"
	},
	"layers": {
		"layer0": {
			"include": "#bottom",
			"submeta": {
				"prefix": "bottom",
				"parent": "#fle:fluidcontainer"
			}
		},
		"layer1": {
			"include": {
				"converts": [ "convert" ],
				"textures": [ "fluid", "empty" ]
			},
			"convert": "#fle:fluidcontainer",
			"submeta": "#fle:fluidcontainer_getfluid",
			"colorMultiplier": "fle:fluidcontainer/fluidcolor"
		}
	}
}