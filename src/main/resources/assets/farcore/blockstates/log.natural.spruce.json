{
	"multipart": [
		{
			"when": {
				"axis": "none"
			},
			"apply": {
				"model": "minecraft:log/spruce"
			}
		},
		{
			"when": {
				"axis": "x"
			},
			"apply": {
				"model": "minecraft:log/spruce",
				"x": 90,
				"y": 90
			}
		},
		{
			"when": {
				"axis": "y"
			},
			"apply": {
				"model": "minecraft:log/spruce",
				"x": 0,
				"y": 0
			}
		},
		{
			"when": {
				"axis": "z"
			},
			"apply": {
				"model": "minecraft:log/spruce",
				"x": 90,
				"y": 0
			}
		}
	]
}